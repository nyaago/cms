  <%= site_stylesheet_link_tags :media => :all %>
  
  <style type="text/css">
  <!--
  -->
  </style>
  
</head>
<body>
    
<div id="header">
    <%= render_theme_partial :header %>
    <%= render_eye_catch_if_require :header %>
</div>   <!-- header -->

<div id="container">

<%= render_eye_catch_if_require :container %>

<div id="contents">

<%= render_eye_catch_if_require :contents %>

