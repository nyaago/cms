  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" /> 
  <meta name="robots" content="all" /> 
  <meta name="keywords" content="" /> 
  <meta name="description" content="" /> 
  <meta name="title" content="" /> 
  
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
